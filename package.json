{
  "name": "ynab-buddy",
  "version": "1.2.1",
  "description": "Upload & import CSV files from any bank into YNAB",
  "keywords": [
    "ynab",
    "youneedabudget",
    "csv",
    "import"
  ],
  "main": "dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist/",
    "assets/"
  ],
  "license": "MIT",
  "author": {
    "name": "Niels Maerten",
    "url": "https://github.com/nielsmaerten"
  },
  "homepage": "https://github.com/nielsmaerten/ynab-buddy",
  "repository": {
    "type": "git",
    "url": "https://github.com/nielsmaerten/ynab-buddy"
  },
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "package": "yarn pkg ./dist/index.js --config ./package.json",
    "package:rebuild": "yarn build && yarn package",
    "publish:np": "yarn build && np --no-cleanup --no-yarn && yarn package --compress Brotli",
    "test": "jest",
    "lint:fix": "yarn prettier ./src --write",
    "lint": "yarn prettier ./src --check"
  },
  "pkg": {
    "assets": [
      "assets/config/example.yaml"
    ],
    "scripts": "build/**/*.js",
    "targets": [
      "linux",
      "win",
      "macos"
    ],
    "outputPath": "bin"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.4",
    "@types/js-yaml": "^4.0.5",
    "@types/luxon": "^2.3.2",
    "@types/node": "^18.0.1",
    "@types/prompts": "^2.0.14",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^28.1.2",
    "pkg": "^5.7.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "chalk": "^5.0.1",
    "csv-parse": "^5.2.2",
    "csv-stringify": "^6.1.3",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "luxon": "^2.4.0",
    "minimatch": "^5.1.0",
    "prompts": "^2.4.2",
    "ynab": "^1.28.0"
  }
}
